{
  "name": "github-gql",
  "version": "1.0.1-canary.2",
  "description": "A fully typed GraphQL client for the Github V4 API",
  "author": "Ryan Pate",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "genql --schema ./node_modules/@octokit/graphql-schema/schema.graphql --output ./src/generated",
    "build:tsc": "yarn tsc -p tsconfig.json",
    "build": "vite-node ./scripts/build.ts && yarn build:tsc"
  },
  "devDependencies": {
    "@genql/cli": "^6.3.3",
    "@octokit/graphql-schema": "^15.25.0",
    "esbuild": "^0.23.0",
    "typescript": "^5.5.3",
    "vite-node": "^2.0.3"
  }
}
